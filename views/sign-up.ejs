<% include header %>
	<div class="form-group">
		<form name="signUpForm" action="sign-up" method="post" class="form-group">
		    <label for="Username">Username</label>
			<input name="username" placeholder="Username" class="form-control"><br>
			<label for="Create Password">Create Password</label>
			<input name="password" placeholder="Create Password" class="form-control"><br>
			<input type="submit" id="Sign Up" class="btn btn-primary pull-left" value="Sign Up">
			<a href="sign-in" class="btn btn-primary pull-right">Return to sign in</a>			
		</form>
	</div>
<% include footer %>
